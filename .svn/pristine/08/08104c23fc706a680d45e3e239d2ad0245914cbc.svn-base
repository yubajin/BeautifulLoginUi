            <div class="ai-logonavi-right">
                
                    <div class="ai-logonavi-search">
                        <form>
                            <input class="ai-logonavi-seinput" type="text" required name="keyword" placeholder="search..."/>
                            <input class="ai-logonavi-sesmt slide-right-btn fa  fa-search-minus" type="submit" value="搜索"/>
                        </form>
                    </div>
                

                
                    
                        <a class="ai-logonavi-login" href="/user/login/"><i class='fa fa-user-o'></i>&nbsp;登录</a>
                    
                    <a class="ai-logonavi-register" href="/user/register/"><i class="fa fa-address-card-o"></i>&nbsp;注册</a>
                
            </div>
        </div>
    </div>



    <div class="ai-register-box" data-heigth="0">
        <div class="ai-register-lizi" data-height="0"></div>
        <div class="ai-register-card-login">
            <p class="ai-register-logo"><a href="/">StudyAi</a></p>
            <form class="ai-login-form" id="email_register_form" action="/user/login/" method="post" enctype="multipart/form-data" autocomplete="on">
                <input type='hidden' name='csrfmiddlewaretoken' value='mrOQoqne8de7DsMmrttb5KRrmEx4Ew7j' />
                <input class="ai-logcard-email" id="id_username" maxlength="128" name="username" placeholder="邮箱账号" type="text" />
                
                <input class="ai-logcard-paswd" id="id_password" name="password" placeholder="输入密码" type="password" />
                
                <p class="ai-login-msg"></p>
                <div class="ai-regcard-okreg">
                    <input class="ai-regcard-regin" type="submit"  value="登录"></input>
                    <a class="ai-regcard-regin" href="/user/register/">注册</a>
                </div>
            </form>
            <div class="ai-logcard-sns">
                <a class="ai-logcard-weixin"><i class="fa fa-weixin"></i></a>
                <a class="ai-logcard-qq"><i class="fa fa-qq"></i></a>
                <a class="ai-logcard-github"><i class="fa fa-github"></i></a>
            </div>
            <div class="ai-regcard-okfind">
                <a class="ai-regcard-detail" href="/user/register/">注册</a>
                <a class="ai-regcard-forget" href="/user/findpwd/">找回密码</a>
            </div>
        </div>
    </div>



